{% extends "index.twig" %}

{% block title %}Project{% endblock %}

{% block headerText %}Projects{% endblock %}

{% block content %}

    <h2>{{ project.title }}</h2>

    <ul class="unstyled">
        {% for name, value in project %}
            <li><b>{{ name }}</b>: {{ value }}</li>
        {% endfor %}
    </ul>

{% endblock %}
