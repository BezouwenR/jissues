{
	"page/:alias": {
		"controller": "App\\Text\\Controller\\Page",
		"methods": ["GET"]
	},
	"text": {
		"controller": "App\\Text\\Controller\\Articles",
		"methods": ["GET"]
	},
	"text/add": {
		"controller": "App\\Text\\Controller\\Article\\Add"
	},
	"text/delete/:id": {
		"controller": "App\\Text\\Controller\\Article\\Delete",
		"rules": {
			"id": "(\\d+)"
		}
	},
	"text/edit/:id": {
		"controller": "App\\Text\\Controller\\Article\\Edit",
		"rules": {
			"id": "(\\d+)"
		}
	},
	"text/save": {
		"controller": "App\\Text\\Controller\\Article\\Save"
	}
}
