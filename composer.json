{
    "name": "joomla/tracker",
    "description": "Issue tracking application extending GitHub's issues and pull requests for the Joomla! project.",
    "keywords": ["joomla", "tracker"],
    "homepage": "https://github.com/joomla/jissues",
    "license": "GPL-2.0+",
    "version": "1.0.0-beta",
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "joomla/application": "~1.4",
        "joomla/authentication": "~1.2",
        "joomla/cache": "~2.0@dev",
        "joomla/controller": "~1.1",
        "joomla/database": "~1.4",
        "joomla/date": "~1.1",
        "joomla/di": "~1.1",
        "joomla/event": "~1.1",
        "joomla/filter": "~1.1",
        "joomla/github": "~1.2",
        "joomla/http": "~1.1",
        "joomla/input": "~1.1",
        "joomla/model": "~1.1",
        "joomla/profiler": "~1.1",
        "joomla/registry": "^1.4.5",
        "joomla/renderer": "~2.0@dev",
        "joomla/router": "~1.1",
        "joomla/string": "~1.3",
        "joomla/uri": "~1.1",
        "joomla/utilities": "^1.3.3",
        "joomla/view": "dev-2.0-dev#cc23d22f8f314e3f81e6e52247605a277b91aaa9",
        "symfony/asset": "4.2.*",
        "symfony/http-foundation": "4.2.*",
        "twig/twig": "~2.7",
        "elkuku/console-progressbar": "1.0",
        "codeguy/upload": "1.3.2",
        "filp/whoops": "2.*",
        "league/flysystem": "~1.0",
        "monolog/monolog": "1.*",
        "kint-php/kint": "^2.0",
        "adaptive/php-text-difference": "1.*",
        "pulse00/monolog-parser": "^0.0.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.13",
        "joomla/coding-standards": "dev-master",
        "phpunit/phpunit"  : "~6.0",
        "squizlabs/php_codesniffer": "2.*",
        "sebastian/phpcpd": "*",
        "phploc/phploc": "*"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-php55": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*"
    },
    "autoload": {
        "psr-4": {
            "JTracker\\": "src/JTracker/",
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JTracker\\Tests\\": "tests/tracker/"
        }
    },
    "config": {
        "platform": {
            "php": "7.2.11"
        }
    }
}
